#
# UBW32 board
# ===========
#
# Console on USB
# For Windows, use the driver: http://www.schmalzhaus.com/UBW32/FW/UBW32inf.zip

core pic32mx7
linker bootloader-ubw32
mapping generic

device kernel cpu_khz=80000 bus_khz=80000 invled=E2

device console invled=E3 port=USB
device uartusb

device rdisk invled=E1 invaux=E0
device sd0 port=1 cs=A9
device sd1 port=1 cs=A10

device gpio
