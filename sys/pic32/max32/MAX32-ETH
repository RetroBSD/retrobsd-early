#
# Max32 board with Arduino Ethernet shield
# ========================================
#
# Console on UART1
# SD/MMC card driver on SPI2

core pic32mx7 cpu_khz=80000 bus_khz=80000
mapping max32
linker bootloader

device kernel led=LED

device uartconsole uart=1

device sd0 port=2 cs=4
device gpio
device spi
device adc

