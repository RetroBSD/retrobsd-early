#
# eflightworks DIP board
# ======================
#
# Console on USB
# For Windows, use the driver: http://www.schmalzhaus.com/UBW32/FW/UBW32inf.zip

core pic32mx7
mapping generic
linker bare

device kernel bus_khz=80000 cpu_khz=80000 led=E5

device console port=USB led=E6
device uartusb

device rdisk led=E4 aux=E7
device sd0 port=3 cs=D4

device gpio
