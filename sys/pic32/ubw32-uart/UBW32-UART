#
# UBW32 board
# ===========
#
# Console on UART2

core pic32mx7
linker bootloader-ubw32
mapping generic

device kernel cpu_khz=80000 bus_khz=80000 invled=E2

device console invled=E3 port=UART2
device uart2

device rdisk invled=E1 invswap=E0
device sd0 port=1 cs=A9
device sd1 port=1 cs=A10

device gpio
